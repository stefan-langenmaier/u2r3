<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:ex="http://www.example.org#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
  <owl:NegativePropertyAssertion>
    <owl:targetIndividual rdf:resource="http://www.example.org#p2"/>
    <owl:assertionProperty rdf:resource="http://www.w3.org/2002/07/owl#subPropertyOf"/>
    <owl:sourceIndividual>
      <rdf:Description rdf:about="http://www.example.org#p1">
        <owl:subPropertyOf rdf:resource="http://www.example.org#p2"/>
      </rdf:Description>
    </owl:sourceIndividual>
  </owl:NegativePropertyAssertion>
</rdf:RDF>
